{
  "name": "bookkeeper",
  "version": "1.0.0",
  "description": "Engine for financial bookkeeping",
  "main": "BookKeeper.js",
  "scripts": {
    "lint": "eslint --ext .js src",
    "build": "rm -f BookKeeper.d.ts; webpack --config build/webpack.config.js && babili BookKeeper.js -o BookKeeper.min.js && dts-bundle --name BookKeeper --main src/BookKeeper.d.ts --out ../BookKeeper.d.ts --baseDir src/; rm src/*.d.ts",
    "lint-test": "eslint --ext .js test",
    "test-debug": "tape -r babel-register test/unit/*.js",
    "test": "npm run lint-test && tape -r babel-register test/unit/*.js | faucet"
  },
  "author": {
    "name": "Adam Carheden",
    "email": "adam.carheden@gmail.com"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-babili": "0.0.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "babili-webpack-plugin": "0.0.10",
    "dtsbundler-webpack-plugin": "^1.0.0",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "faucet": "0.0.1",
    "tap-webpack-plugin": "^1.1.0",
    "tape": "^4.6.2",
    "ts-loader": "^2.0.1",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.4.2",
    "typescript": "^2.2.1",
    "webpack": "^1.13.3"
  },
  "dependencies": {}
}
